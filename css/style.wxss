body{
    zoom:100%;
}
.colockbox {
    width: 100%;
    height: 29px;
    padding: 6px 0;
    background: #fff;
    border-bottom: 3px #f4f4f4 solid;
    color: #777;
}
.colockbox p {
    float: left;
    padding-left: 11px;
    padding-right: 5px;
    height: 29px;
    line-height: 29px;
    font-size: 14px;
}
.colockbox span {
    float: left;
    display: block;
    height: 25px;
    line-height: 25px;
    font-size: 13px;
    background: #444;
    font-weight: normal;
    text-align: center;
    color: #fff;
    margin-right: 2px;
    border-radius: 25px;
    width: 25px;
    font-family: "Open Sans", Arial, sans-serif;
}
.colockbox s {
    float: left;
    display: block;
    height: 29px;
    line-height: 29px;
    font-size: 13px;
    color: #777;
    text-decoration: none;
    margin-right: 5px;
}