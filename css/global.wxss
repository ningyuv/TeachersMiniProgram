view{margin:0;padding:0;}
a:active,a:hover{outline:0}
table{border-collapse:collapse;border-spacing:0}
td,th{padding:0}
image{vertical-align:middle;border:0}
body{font-family:"微软雅黑";font-size:.28em;color:#333;background-color:#f1f1f1;}
ul li{margin:0;padding:0;list-style-type:none}
a{color:#666;text-decoration:none;}
a:hover{color:#FC605A;}
html{-webkit-tap-highlight-color:transparent;height:100%;overflow-x:hidden;}
header{height:45px;background:#FC605A;position:relative;width:100%;float:left;font-size:20px;}
.clear{ clear:both;}
.fixed{ position:fixed;top:0;width:100%; z-index:999;}
.h45{height:45px;}

view::-webkit-scrollbar {
    width: 5px;
    background-color: #F5F5F5;
}
view::-webkit-scrollbar-thumb {
    border-radius: 5px;
    -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,.3);
    background-color: #ddd;
}
view::-webkit-scrollbar-track {
    -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3);
    border-radius: 5px;
    background-color: #F5F5F5;
}


/*下拉选择*/
.headertwo{width: 100%; background-color: #f58611; position: fixed; top: 0; left: 0; z-index:99;}
.headertwo a{display: block; text-align: center; padding-right: .3rem;}
.headertwo a:first-child{width: 10%; padding-left: .3rem; padding-right: 0;}
.headertwo p{width: 80%; text-align: center; color: #fff; font-size: .5rem;}
.headertwo a i{color: #fff; font-size: .5rem;}
.headertwo .bt{width: 76%;}

.recomtwo{margin-top: 1.5rem;}
.menu-list{width: 100%; position: fixed; z-index: 99;}
.menu-list ul li{background-color:#fff; text-align: center; width:50%;float: left; }
.yiji{ position:relative;}
.menu-list ul li a{color: #888888; border-bottom:solid 1px #dfdfdf; font-size:.36rem; display: block; position: relative; padding: .3rem 0;}
.menu-list ul li .inactive i{ background:url(../images/xia.png) no-repeat center; background-size: 50%; width: 29px; height: 8px; display: inline-block;}
.menu-list ul li .inactives i{background:url(../images/shang.png) no-repeat center; background-size: 50%; width: 29px; height: 8px; display: inline-block;} 
.menu-list ul li .inactives{color: #f58611;}
.menu-list ul li ul{display: none;width:100%; position:absolute;left:0;background:#fff;}
.menu-list ul li ul li { border-left:0; text-align: center; border-right:0; background-color:#fff; border-color:#dfdfdf;width:25%;}
.menu-list ul li ul li a{display: block; color:#333333; font-size: .36rem;}
.menu-last{ background-color:#d6e6f1; border-color:#6196bb;}
.menu-list ul li ul li:nth-child(2n){background-color: #fafafa;}


.am-sharetwo{z-index: 998;}
.sharebgtwo { background-color: rgba(0, 0, 0, 0.6); top: 0; height: 100%; left: 0; opacity: 0; position: fixed; right: 0; top: 0; width: 100%; z-index: 10000; display:none; }
.sharebg-active { opacity: 1; display:block; }

.recomtwo image{width: 100%;}
.recome-close{width: 100%; padding: 3% 3% 0 3%;}
.recome-close p{font-size: .4rem; color: #333; width: 70%; overflow: hidden; text-overflow: ellipsis; -webkit-text-overflow: ellipsis; white-space: nowrap;}
.oe_over {background:rgba(0,0,0,0.7);position:fixed;top: 0px;left: 0px;width: 100%;height: 100%; z-index:99;}



/*底部*/
.height{height:55px;clear:both;}
.aui-badge {display: inline-block;width: auto;text-align: center;min-width: 0.5rem;height: 0.5rem;line-height: 0.5rem;padding: 0 0.02rem;font-size: 0.3rem;color: #ffffff;background-color: #ff2600;
border-radius: 0.4rem;position: absolute;top: 0.2rem;left: 60%;z-index: 99;font-family: "Open Sans", Arial, sans-serif;}
.aui-dot {display: inline-block;width:8px;height:8px;background: #ff2600;position: absolute;top: 0.4rem;right:30%;z-index: 99;border-radius:8px;}
.aui-bar {position: relative;top: 0;right: 0;left: 0;z-index:999;width: 100%;font-size: 0.9rem;text-align: center;display: table;}
.aui-bar-tab {position: fixed;top: auto;bottom: 0;table-layout: fixed;background-color: #ffffff;color: #757575;padding:1px 0;border-top:1px #ddd solid;}
.aui-bar-tab .aui-bar-tab-item {display: table-cell;position: relative;width: 1%;text-align: center;vertical-align: middle;}
.aui-bar-tab .aui-active {color: #FC605A;}
